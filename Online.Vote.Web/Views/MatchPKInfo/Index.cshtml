@model IEnumerable<Online.Vote.Domain.MatchPKInfo>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FirstSongName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SecondSongName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FirstPlayerScore)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SecondPlayerScore)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MatchFlag)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstSongName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SecondSongName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstPlayerScore)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SecondPlayerScore)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MatchFlag)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
                @Html.ActionLink("Details", "Details", new { id=item.ID }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.ID })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
